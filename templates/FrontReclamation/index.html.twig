    <!-- Map Section End -->

    <!-- Contact Section Begin -->
    <section class="contact-section spad">
        <div class="container">
            <div class="row">
                <div class="col-lg-4">
                    <div class="contact-info">
                        <h4>Contactez Nous</h4>
                        <div class="contact-address">
                            <div class="ca-widget">
                                <div class="cw-icon">
                                    <img src="front/img/icon/icon-1.png" alt="">
                                </div>
                                <div class="cw-text">
                                    <h5>Adresse</h5>
                                    <p>Ghazela-Ariana</p>
                                </div>
                            </div>
                            <div class="ca-widget">
                                <div class="cw-icon">
                                    <img src="front/img/icon/icon-2.png" alt="">
