}

.event-location {
  font-size: 18px;
  margin-bottom: 20px;
  color: #000000;
}

.event-description {
  font-size: 16px;
  line-height: 1.6;
  margin-bottom: 20px;
  color: #5a5450;
}
.breadcrumb-text h2 {
  color: blue;
}

.breadcrumb-option a {
  color: green;
}
.event-title {
  font-size: 40px;
